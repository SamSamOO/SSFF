<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ssff.studyIns.mapper.StudyInsMapper">
    <select id="getList" resultType="kr.or.ssff.studyIns.domain.StudyInsVO">
        select cont_no, category, title, cont, member_name, write_date
        from si_board
        where cont_no > 0
        ORDER BY cont_no desc;
    </select>

</mapper>


